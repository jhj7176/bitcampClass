			for (int i = 1; i < 10; i++) {
				int dan = 2;
		%>

		<tr align="center">
			<%
				for (int j = 1; j < 9; j++) {
			%>
			<td height="60"><%=dan%> x <%=i%> = <%=dan * i%></td>

			<%
				dan++;
					}//for
			%>
		</tr>
